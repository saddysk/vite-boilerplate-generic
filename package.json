{
  "name": "1811labs",
  "private": true,
  "version": "0.0.1-beta.33",
  "type": "module",
  "author": "1811 Labs",
  "homepage": "https://1811labs.com",
  "scripts": {
    "dev": "cross-env PORT=3000 vite --host=0.0.0.0",
    "build": "npm run sync:constants && tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare-vercel": "fse move --overwrite vercel.$VERCEL_ENV.json vercel.json",
    "validate": "run-s format lint typecheck",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx --cache --fix",
    "format": "prettier --write \"**/*.{ts,tsx,css,html,js,jsx,json}\"",
    "prepare": "husky install",
    "generate:api": "fse move ./src/api/http-client.ts ./src/api/http-client.ts.bak && curl http://localhost:3001/api/doc-json --output ./resources/.swagger.json && swagger-typescript-api -p ./resources/.swagger.json -o ./src/api --axios --no-client --modular --module-name-first-tag && fse move --overwrite ./src/api/http-client.ts.bak ./src/api/http-client.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1811labs/1811labs.git"
  },
  "keywords": [
    "ai"
  ],
  "engines": {
    "node": "22"
  },
