    "classnames": "^2.3.1",
    "clsx": "^2.1.1",
    "constate": "^3.3.2",
    "copy-to-clipboard": "^3.3.2",
    "date-fns": "^2.29.1",
    "framer-motion": "^7.5.1",
    "html-entities": "^2.3.3",
